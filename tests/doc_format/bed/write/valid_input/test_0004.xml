<!-- Verify saving of a valid BED file: only 5 fields. -->

<multi-test lockForLogListening="true">

    <load-document index="doc" url="bed/valid_input/fields_num_5.bed" io="local_file" format="bed" no-message="BED parsing error"/>

    <save-document doc="doc" url="_tmp_fields_num_5.bed" io="local_file" format="bed"/>

