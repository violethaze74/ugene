<!-- circ-circ [joining] -->
<!-- Use megablast -->
<multi-test>
    <run-cmdline
            task="remote-request"
            in="!common_data_dir!cmdline/remote-request/pIB2_circ.gb"
            out="!tmp_data_dir!remote-request-circ-0001.gb"
            alg="ncbi-blastn"
            megablast="true"
    />
    <load-document index="doc1" url="remote-request-circ-0001.gb" io="local_file" format="genbank" dir="temp"/>
    <load-document index="doc2" url="cmdline/remote-request/circular/01.gb" io="local_file" format="genbank"/>

    <compare-annotations-num-in-two-objects doc="doc1" value="doc2"/>
    <compare-annotations-locations-in-two-objects doc="doc1" value="doc2"/>
</multi-test>
