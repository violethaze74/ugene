<multi-test>

    <!-- MrBayes Test -->
    <load-document index="doc1" url="phyml/shortDNA.phy" io="local_file" format="phylip-interleaved"/>
    <load-document index="doc2" url="phyml/shortDNA_GTR.nwk" io="local_file" format="newick"/>
    <test-PHYML in="doc1" sample="doc2" model="GTR" bootstrap="-2"/>

</multi-test>
